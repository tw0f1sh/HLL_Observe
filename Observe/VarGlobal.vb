Imports System.IO
Imports System.Media
Imports System.Text
Module VarGlobal
    Public ntpInstance As New Ntimes()
    Public SyncedTimeOffset As TimeSpan = TimeSpan.Zero

    Public ReadOnly Property SyncedTime As DateTime
        Get
            Return DateTime.Now + SyncedTimeOffset
        End Get
    End Property

    Public WebVideoCreatorEnable As Boolean = False 'enable the VideoCreatorAuthUrl instead of the offline video creator form

    Public changelogURL As String = "https://google.de" 'Link to changelog.txt
    Public uploadURL As String = "https://google.de" 'Link to the auth.php from the upload file server 
    Public VideoCreatorAuthUrl As String = "https://google.de" 'Link to the auth.php from the Webbased Video Creator
    Public UpdateExeUrl As String = "https://google.de" 'Link to the .exe of the Updater
    Public UpdaterVersionUrl As String = "https://google.de" 'Link to *.txt where u set the UPDATER live version number
    Public AppVersionUrl As String = "https://google.de" 'Link to *.txt where u set the APP/OBSERVE live version number
    Public DiscordURL As String = "https://google.de" 'Link to your Discord

    Public SaveTeachinImage As Boolean = False 'Saves an Debug Image where the Offsetpoints drawn to 
    Public EnableBeep As Boolean = False 'On each picture taken the app will Beep



    Public TXTVersion As String = "0.8.26"

    Public objVersionApp As New Version(TXTVersion)


    Public TXTOffsetPoints As String = ""
    Public TXTSnapSize As String = ""
    Public TXTSnapTaken As Integer = 0
    Public TXTResolution As String = ""

    Public INIPath As String = Path.Combine(Application.StartupPath + "settings.ini")


    Public Interval As Integer = 250
    Public timeout_between_save As Integer = 5000
    Public Threshold As Integer = 8 'befor: 5 'Threshold: Farbabgleich
    Public MinLenth As Integer = 30 'MinRun: Mindestlänge einer Linie
    Public LineSpacing As Integer = 5 'Separation : Abstand, um nahe Linien zusammenzufassen
    Public xDetectionTolerance As Integer = 5 'Tolerance : Toleranz, um den Schnittpunkt in der Mitte zu erkennen

    Public output_map_scale As New Size(800, 800)


    Public beepBase64 As String = ""
    Function WAVbeep()
        Dim beepBytes As Byte() = Convert.FromBase64String(beepBase64)
        Using ms As New MemoryStream(beepBytes)
            Using sp As New SoundPlayer(ms)
                sp.Play()
            End Using
        End Using
        Return 0
    End Function



End Module
